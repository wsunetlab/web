body {
  background:grey;
  font-family:Verdana, Sans-Serif;
}

#content {
  margin-right:2%;
  margin-left:2%;
  margin-top:2%;
  padding-bottom:10px;
  background:#FFF;
  border:1px solid #000;
  text-align:left;
  min-width:500px;
}

p, h1, h2, form {
  margin-top:0px;
  padding:0px 20px 0px 20px;
  margin-bottom:10px;
}

#menu {
  width:240px;
  padding:0px;
  margin-left:20px;
  margin-right:-3px;
  float:right;
}

html>body #menu {
  margin-right:-6px;
}

.menus {
  border-bottom:1px solid #000;
  border-left:1px solid #000;
  border-right:10px solid #000;
  border-top:1px solid #000;
  background:#FFF;
  color:#000;
  padding:10px;
  margin-top:10px;
  text-align:left;
}

#menulogin {
  padding-bottom:0px;
  padding-left:0px;
}

#menulogin table {
  width:100%;
  font-size:10px;
}

#title {
  padding:0px;
  margin-right:-6px;
  margin-left:-6px;
  margin-top:10px;
  font-size:18px;
  border-top:1px solid #000;
  border-bottom:1px solid #000;
  border-right:10px solid #000;
  border-left:10px solid #000;
  height:65px;
  color:#800000;
  margin-bottom:5px;
}

#titleimage {
  float:left;
}

#titlemain {
  float:left;
  font-family: Verdana, Sans-Serif;
  margin-top:5px;
  margin-right:10px;
  font-size:14;
  text-align:left;
  padding-left:10px;
  padding-right:10px;
}

#titletop {
  padding:0px 0px 0px 0px;
  margin:0px 0px 0px 0px;
  height:45px;
}

#titlebottom {
  padding:0px 0px 0px 0px;
  margin:0px 0px 0px 0px;
  height:10px;
}

.titleelementright {
  float:right;
  font-family: Verdana, Sans-Serif;
  margin-right:10px;
  font-size:13;
  text-align:right;
}

a.titlelink {
  color:#800000;
}

a.titleauthlink {
  color:#4599ff;
}

a.titleadminlink {
  color:red;
}

#tabbar {
  padding:0px;
  height:20px;
  margin-left:20px;
  font-family:Verdana, Sans-Serif;
} 

.tabs {
  float:left;
  padding-right:10px;
  padding-left:10px;
  width:auto;
  border-top:1px solid #000;
  border-left:1px solid #000;
  border-right:1px solid #000;
  border-bottom:1px solid #000;
  margin-right:-1px;
  margin-bottom:-1px;
  cursor:pointer;
}

.panels {
  padding:10px;
  border:1px solid #000;
  min-height:300px;
}

.scheduleFree {
  background-color:#0040e3;
}

.scheduleFreeHighlight {
  background-color:#6589e3;
}

.scheduleFreeSelected {
  background-color:#122e75;
}

.scheduleFreeNotSelected {
  background-color:grey;
}

.scheduleFinished {
  background-color:#00b3ff;
}

.scheduleFinishedHidden {
  background-color:grey;
}

.scheduleRunning {
  background-color:#ff1a00;
}

.schedulePending {
  background-color:#fffc31;
}

.scheduleUnavailable {
  background-color:#00b3ff;
}

.jobFinished {
  color:white;
}

.jobDeleted {
  color:grey;
}

.jobRunning {
  color:#ff9292;
}

.jobPending {
  color:#f0f020;
}

.error {
  color:red;
}

.formRequired {
  color:red;
}

.formFulfilled {
  color:green;
}

.formOptional {
  color:blue;
}
